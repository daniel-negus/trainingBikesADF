{
	"name": "BDHdevBakery",
	"properties": {
		"activities": [
			{
				"name": "BDHdevBakery",
				"description": "Copy records from Profisee via the Profisee REST API to ADLS Gen2 in CSV format.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"AbsoluteUrl": "$.nextPage"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "internalid"
								},
								"sink": {
									"name": "internalId",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "id"
								},
								"sink": {
									"name": "id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "code"
								},
								"sink": {
									"name": "code",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "name"
								},
								"sink": {
									"name": "name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "bakeryNumber"
								},
								"sink": {
									"name": "bakeryNumber",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "franchisee"
								},
								"sink": {
									"name": "franchisee",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "region"
								},
								"sink": {
									"name": "region",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "supplierRegion"
								},
								"sink": {
									"name": "supplierRegion",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "areaManagerName"
								},
								"sink": {
									"name": "areaManagerName",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "bakeryForSale"
								},
								"sink": {
									"name": "bakeryForSale",
									"type": "Boolean"
								}
							},
							{
								"source": {
									"path": "storeType"
								},
								"sink": {
									"name": "storeType",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "dateOpened"
								},
								"sink": {
									"name": "dateOpened",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "dateClosed"
								},
								"sink": {
									"name": "dateClosed",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "royaltyPercentage"
								},
								"sink": {
									"name": "royaltyPercentage",
									"type": "Decimal"
								}
							},
							{
								"source": {
									"path": "currency"
								},
								"sink": {
									"name": "currency",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "menulogURL"
								},
								"sink": {
									"name": "menulogURL",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "uberURL"
								},
								"sink": {
									"name": "uberURL",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['data']",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "BDHdevGET",
						"type": "DatasetReference",
						"parameters": {
							"entityId": "@variables('EntityId')",
							"pageSize": 1000,
							"attributes": " ",
							"filter": " ",
							"orderBy": " ",
							"dbaFormat": 1,
							"recordCodes": " "
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AdlsGen2BlobCsv2",
						"type": "DatasetReference",
						"parameters": {
							"FolderName": "Bakery",
							"FileName": "Bakery"
						}
					}
				]
			}
		],
		"variables": {
			"OutputContainer": {
				"type": "String",
				"defaultValue": "profisee-output"
			},
			"EntityId": {
				"type": "String",
				"defaultValue": "Bakery"
			}
		},
		"folder": {
			"name": "CSV Format"
		},
		"annotations": [],
		"lastPublishTime": "2020-09-25T15:29:08Z"
	}
}