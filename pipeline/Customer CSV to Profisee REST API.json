{
	"name": "Customer CSV to Profisee REST API",
	"properties": {
		"activities": [
			{
				"name": "Copy from CSV to Profisee REST API",
				"description": "Copy records from ADLS Gen2 in CSV format to Profisee via the Profisee REST API.",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "RestSink",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": 10,
						"requestMethod": "PATCH",
						"writeBatchSize": 10000,
						"httpCompressionType": "none"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Name",
									"type": "String"
								},
								"sink": {
									"path": "$['Name']"
								}
							},
							{
								"source": {
									"name": "Code",
									"type": "String"
								},
								"sink": {
									"path": "$['Code']"
								}
							},
							{
								"source": {
									"name": "Source System",
									"type": "String"
								},
								"sink": {
									"path": "$['SourceSystem']"
								}
							},
							{
								"source": {
									"name": "Record Source Code",
									"type": "String"
								},
								"sink": {
									"path": "$['RecordSourceCode']"
								}
							},
							{
								"source": {
									"name": "Salutation",
									"type": "String"
								},
								"sink": {
									"path": "$['Salutation']"
								}
							},
							{
								"source": {
									"name": "Legal Name",
									"type": "String"
								},
								"sink": {
									"path": "$['LegalName']"
								}
							},
							{
								"source": {
									"name": "Preferred Name",
									"type": "String"
								},
								"sink": {
									"path": "$['Preferred Nam']"
								}
							},
							{
								"source": {
									"name": "First Name",
									"type": "String"
								},
								"sink": {
									"path": "$['FirstName']"
								}
							},
							{
								"source": {
									"name": "Last Name",
									"type": "String"
								},
								"sink": {
									"path": "$['LastName']"
								}
							},
							{
								"source": {
									"name": "Date Of Birth",
									"type": "String"
								},
								"sink": {
									"path": "$['DateOfBirth']"
								}
							},
							{
								"source": {
									"name": "Primary Email Address",
									"type": "String"
								},
								"sink": {
									"path": "$['PrimaryEmailAddress']"
								}
							},
							{
								"source": {
									"name": "Primary Mobile Phone Number",
									"type": "String"
								},
								"sink": {
									"path": "$['PrimaryMobilePhoneNumber']"
								}
							},
							{
								"source": {
									"name": "Primary Home Phone Number",
									"type": "String"
								},
								"sink": {
									"path": "$['PrimaryHomePhoneNumber']"
								}
							},
							{
								"source": {
									"name": "Primary Business Phone Number",
									"type": "String"
								},
								"sink": {
									"path": "$['PrimaryBusinessPhoneNumber']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "AdlsGen2BlobCsv3",
						"type": "DatasetReference",
						"parameters": {
							"FolderName": "profisee-input",
							"FileName": "customer"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ProfiseeRESTAPI_Records_Upsert1",
						"type": "DatasetReference",
						"parameters": {
							"entityId": "Customer"
						}
					}
				]
			}
		],
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "customer"
			},
			"FileFolder": {
				"type": "string",
				"defaultValue": "profisee-input"
			}
		},
		"folder": {
			"name": "CSV Format"
		},
		"annotations": [],
		"lastPublishTime": "2020-09-25T15:29:08Z"
	}
}