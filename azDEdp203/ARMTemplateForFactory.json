										"source": {
											"path": "[['region']"
										},
										"sink": {
											"name": "region",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "[['supplierRegion']"
										},
										"sink": {
											"name": "supplierRegion",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "[['areaManagerName']"
										},
										"sink": {
											"name": "areaManagerName",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "[['bakeryForSale']"
										},
										"sink": {
											"name": "bakeryForSale",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "[['storeType']"
										},
										"sink": {
											"name": "storeType",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "[['dateOpened']"
										},
										"sink": {
											"name": "dateOpened",
											"type": "DateTime"
										}
									},
									{
										"source": {
											"path": "[['dateClosed']"
										},
										"sink": {
											"name": "dateClosed",
											"type": "DateTime"
										}
									},
									{
										"source": {
											"path": "[['royaltyPercentage']"
										},
										"sink": {
											"name": "royaltyPercentage",
											"type": "Decimal"
										}
									},
									{
										"source": {
											"path": "[['currency']"
										},
										"sink": {
											"name": "currency",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "[['menulogURL']"
										},
										"sink": {
											"name": "menulogURL",
											"type": "String"
										}
									},
									{
										"source": {
											"path": "[['uberURL']"
										},
										"sink": {
											"name": "uberURL",
											"type": "String"
										}
									}
								],
								"collectionReference": "$['data']",
